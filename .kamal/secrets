SECRETS=$(kamal secrets fetch --adapter 1password --account $OP_ACCOUNT --from Deploy/SFBUFF/Production)

SFBUFF_SECRET_KEY_BASE=$(kamal secrets extract SFBUFF_SECRET_KEY_BASE $SECRETS)
SFBUFF_KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract SFBUFF_KAMAL_REGISTRY_PASSWORD $SECRETS)
SFBUFF_SENTRY_DSN=$(kamal secrets extract SFBUFF_SENTRY_DSN $SECRETS)
SFBUFF_BUCKLER_EMAIL=$(kamal secrets extract SFBUFF_BUCKLER_EMAIL $SECRETS)
SFBUFF_BUCKLER_PASSWORD=$(kamal secrets extract SFBUFF_BUCKLER_PASSWORD $SECRETS)
SFBUFF_DEFAULT_USER_AGENT=$(kamal secrets extract SFBUFF_DEFAULT_USER_AGENT $SECRETS)
SFBUFF_DATABASE_PASSWORD=$(kamal secrets extract SFBUFF_DATABASE_PASSWORD $SECRETS)
